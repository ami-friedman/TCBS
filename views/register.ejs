<% include ./partials/header.ejs %>

<form action="/register" method="POST">
    <input type="text" name="user[name][first]" placeholder="First Name" required>
    <input type="text" name="user[name][last]" placeholder="Last Name" required>
    <input type="email" name="user[email]" placeholder="E-mail" required>
    <input type="password" name="user[password]" placeholder="Password" required>
    <input type="password" name="user[passwordConfirm]" placeholder="Confrim Password" required>
    <input type="submit" value="Register">
</form>

